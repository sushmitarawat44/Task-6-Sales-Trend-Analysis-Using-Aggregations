      Select * From Online_Sales;
      
SELECT 
    strftime('%Y', order_date) AS year,         -- Extract year
    strftime('%m', order_date) AS month,        -- Extract month
    SUM(amount) AS total_revenue,               -- Total monthly revenue
    COUNT(DISTINCT customer_id || order_date) AS order_volume  -- Approx. unique orders
FROM Online_Sales
WHERE order_date IS NOT NULL
GROUP BY 
    strftime('%Y', order_date),
    strftime('%m', order_date)
ORDER BY 
    year, month
LIMIT 10;
 
